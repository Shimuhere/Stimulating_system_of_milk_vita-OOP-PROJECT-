      <Label layoutX="15.0" layoutY="11.0" prefHeight="38.0" prefWidth="179.0" text="Material Stock Information" />
      <Button layoutX="50.0" layoutY="329.0" mnemonicParsing="false" onAction="#backHandle" text="Back" />
